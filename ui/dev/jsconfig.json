{
  "compilerOptions": {
    "baseUrl": ".",

    "paths": {
      "src/*": ["src/*"],
      "app/*": ["*"],
      "components/*": ["src/components/*"],
      "helpers/*": ["src/helpers/*"],
      "layouts/*": ["src/layouts/*"],
      "mixins/*": ["src/mixins/*"],
      "pages/*": ["src/pages/*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"],
      "vue$": ["node_modules/vue/dist/vue.esm.js"]
    }
  },

  "exclude": [
    ".quasar",
    "dist",
    "node_modules"
  ]
}
