    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-quasar": "^1.1.0",
    "eslint-plugin-vue": "^9.12.0",
    "execa": "^7.1.1",
